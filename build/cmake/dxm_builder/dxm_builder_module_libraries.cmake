

# 
# 加载模块链接库
# 
macro(_dxm_builder_module_link_libraries)
	# 链接库
	LIST(LENGTH DXM_MODULE_${DXM_BUILDER_MODULE}_LIBRARIES LEN)
	# MESSAGE(STATUS "======= TARGET_LINK_LIBRARIES(${DXM_BUILDER_MODULE} ${DXM_MODULE_${DXM_BUILDER_MODULE}_LIBRARIES}) ${LEN}")
	IF(NOT LEN EQUAL 0)
		# MESSAGE(STATUS "======= TARGET_LINK_LIBRARIES(${DXM_BUILDER_MODULE} ${DXM_MODULE_${DXM_BUILDER_MODULE}_LIBRARIES}) ${LEN}")
		TARGET_LINK_LIBRARIES(${DXM_BUILDER_MODULE} ${DXM_MODULE_${DXM_BUILDER_MODULE}_LIBRARIES})
	ENDIF()
endmacro()